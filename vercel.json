{
  "version": 2,
  "routes": [
    { "src": "/login", "dest": "/api/proxy" },
    { "src": "/admin", "dest": "/api/proxy" },
    { "src": "/akun", "dest": "/api/proxy" },
    { "src": "/ml", "dest": "/api/proxy" },
    { "src": "/api/(.*)", "dest": "/api/proxy" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
